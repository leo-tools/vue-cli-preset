{
  "useTaobaoRegistry": false,
  "packageManager": "yarn",
  "useConfigFiles": true,
  "router": true,
  "vuex": true,
  "cssPreprocessor": "node-sass",
  "plugins": {
    "@vue/cli-plugin-babel": {},
    "@vue/cli-plugin-pwa": {},
    "@vue/cli-plugin-router": {
      "historyMode": true
    },
    "@vue/cli-plugin-vuex": {},
    "@vue/cli-plugin-eslint": {
      "config": "prettier",
      "lintOn": [
        "save"
      ]
    },
    "@vue/cli-plugin-e2e-cypress": {},
    "@vue/cli-plugin-unit-jest": {}
  }
}
